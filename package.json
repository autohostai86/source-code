{
  "name": "nx-env",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build:client": "nx build client --production --optimize",
    "build:chatapp": "nx build chatapp --production --optimize",
    "build:api": "nx build api --production --optimize --generatePackageJson true",
    "test": "nx test",
    "docker:build-up": "sudo docker-compose -f docker-compose-dev.yml up -d --build",
    "open": "code mocr.code-workspace",
    "docker:up": "sudo docker-compose -f docker-compose-dev.yml up -d ",
    "docker:down": "docker-compose -f docker-compose-dev.yml down ",
    "docker:project-install": "yarn docker:up && sudo docker exec -w /usr/src/app -it node-chat yarn install ",
    "docker:project-add": "yarn docker:up && sudo docker exec -w /usr/src/app -it node-chat yarn add ",
    "docker:client-start": "yarn docker:up &&  docker exec -w /usr/src/app -it  node-chat  nx serve client --host 0.0.0.0 --port 3000",
    "docker:chatapp-start": "yarn docker:up &&  docker exec -w /usr/src/app -it node-chat nx serve chatapp --host 0.0.0.0 --port 3001",
    "docker:client-build": "yarn docker:up &&  docker exec -w /usr/src/app -i node-chat /bin/bash -c 'nx build client'",
    "docker:server-start": "yarn docker:up &&  docker exec -w /usr/src/app/apps/api -it node-chat nx serve api --host 0.0.0.0 ",
    "docker:server-build": "yarn docker:up &&  docker exec -w /usr/src/app -i node-chat  nx build api",
    "---": "---------------------- prepare commands -----------------------------",
    "docker:husky-init": "docker exec -w /usr/src/app -i node-chat npx husky-init",
    "prepare:project": "yarn docker:project-install && yarn docker:husky-init",
    "prepare": "cp -rf configs/husky/pre-commit .husky && chmod +x ./.husky/pre-commit &&  husky install",
    "----": "---------------------- windows commands -----------------------------",
    "docker-win:build-up": "docker-compose -f docker-compose-dev.yml up -d --build",
    "docker-win:up": "docker-compose -f docker-compose-dev.yml up -d ",
    "docker-win:down": "docker-compose -f docker-compose-dev.yml down ",
    "docker-win:project-install": "yarn docker-win:up && docker exec -w /usr/src/app -it node-chat yarn install ",
    "docker-win:project-add": "yarn docker-win:up && docker exec -w /usr/src/app -it node-chat yarn add ",
    "docker-win:client-start": "yarn docker-win:up &&  docker exec -w /usr/src/app -it node-chat nx serve client --host 0.0.0.0 --port 3000",
    "docker-win:chatapp-start": "yarn docker-win:up &&  docker exec -w /usr/src/app -it node-chat nx serve chatapp --host 0.0.0.0 --port 3001",
    "docker-win:client-build": "yarn docker-win:up &&  docker exec -w /usr/src/app -i node-chat /bin/bash -c 'nx build client'",
    "docker-win:server-start": "yarn docker-win:up &&  docker exec -w /usr/src/app/apps/api -it node-chat nx serve api --host 0.0.0.0 ",
    "docker-win:server-build": "yarn docker-win:up &&  docker exec -w /usr/src/app -i node-chat  nx build api",
    "------": "---------------------- windows commands -----------------------------",
    "docker-win:husky-init": "docker exec -w /usr/src/app -i node-chat npx husky-init",
    "prepare-win:project": "yarn docker-win:project-install && yarn docker-win:husky-init",
    "prepare-win": "husky install"
  },
  "lint-staged": {
    "apps/client/**/*.{ts,tsx}": [
      "eslint --quiet --fix "
